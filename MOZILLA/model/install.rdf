<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <RDF:Description about="urn:mozilla:install-manifest">
  <!-- https://developer.mozilla.org/en/Install.rdf -->

    <em:id>pt-PT@dictionaries.addons.mozilla.org</em:id>
    <em:type>2</em:type>
    <em:version><% version %></em:version>
    <em:name>Corretor para Português de Portugal</em:name>
    <em:description>Portuguese, Portugal (pt-PT<% prefix %>) Dictionary</em:description>
    <em:unpack>true</em:unpack>

    <em:creator>Projecto Natura</em:creator>
    <em:developer>Alberto Simões</em:developer>
    <em:homepageURL>http://natura.di.uminho.pt/wiki/doku.php?id=dicionarios:main</em:homepageURL>

    <em:contributor>António Manuel Dias</em:contributor>
    <em:contributor>José João de Almeida</em:contributor>
    <em:contributor>Rui Vilela</em:contributor>

    <em:localized>
      <RDF:Description>
        <em:locale>pt-PT</em:locale>
        <em:name>Corretor para Português de Portugal</em:name>
        <em:description>Corretor ortográfico para português de Portugal (pt-PT<% prefix %>)</em:description>
        <em:creator>Projecto Natura</em:creator>
        <em:developer>ALberto Simões</em:developer>
        <em:contributor>António Manuel Dias</em:contributor>
        <em:contributor>José João de Almeida</em:contributor>
        <em:contributor>Rui Vilela</em:contributor>
        <em:homepageURL>http://natura.di.uminho.pt/wiki/doku.php?id=dicionarios:main</em:homepageURL>
      </RDF:Description>
    </em:localized>

    <!-- toolkit -->
    <em:targetApplication>
      <RDF:Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>1.9</em:minVersion>
        <em:maxVersion><% toolkit_maxversion %></em:maxVersion>
      </RDF:Description>
    </em:targetApplication>

    <!-- firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion><% firefox_maxversion %></em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion><% thunder_maxversion %></em:maxVersion>
      </Description>
    </em:targetApplication>

  </RDF:Description>
</RDF:RDF>
